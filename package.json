{
  "name": "parse-audio-metadata",
  "version": "1.0.3",
  "description": "Audio file metadata parser for browser",
  "main": "dist/parseAudioMetadata.js",
  "keywords": [
    "audio",
    "metadata",
    "parser",
    "mp3",
    "flac",
    "opus",
    "ogg",
    "m4a"
  ],
  "author": "andriuskv",
  "license": "MIT",
  "scripts": {
    "start": "npm run dist-dev && npm run watch & live-server dist",
    "dist": "rm -rf dist && mkdir dist",
    "dist-dev": "npm run dist && cp src/index.html dist",
    "watch": "rollup -cw",
    "build": "npm run dist && rollup -c"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "live-server": "^1.2.1",
    "rollup": "^1.25.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "browserslist": [
    "last 4 chrome versions",
    "last 4 firefox versions",
    "last 1 edge versions",
    "last 1 and_chr versions",
    "last 1 safari versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andriuskv/parse-audio-metadata.git"
  }
}
